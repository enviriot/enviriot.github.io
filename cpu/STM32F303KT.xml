<?xml version="1.0" encoding="utf-8"?>
<cpu name="S4" ainref="0x02">
    <!-- ainref
        Bit0 - Ext('e'), Bit1 - Vcc('v'), Bit2 - Int1('i'), Bit3 - Int2('I')
    -->
    <!-- pin 
            fpin    - номер ноги контроллера
    -->
    <!-- pwm  
            timer   - номер таймера
            channel - канал таймер
            af      - номер альтернативной функции
            -->
