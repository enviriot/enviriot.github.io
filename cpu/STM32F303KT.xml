  <port name="GPIOF" titel="PF" offset="80" nr="5" pinset="GPIOF-&gt;BSRR = (1 &lt;&lt; {0})" pinrst="GPIOF-&gt;BRR = (1 &lt;&lt; {0})" pinget="GPIOF-&gt;IDR &amp; (1 &lt;&lt; {0})">
    <pin idx="0" fpin="02">
