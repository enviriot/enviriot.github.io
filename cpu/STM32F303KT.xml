    </pin>
    <pin idx="4" fpin="27">
      <dio />
      <pwm timer="16"  channel="0" af="1"/>
      <pwm timer="3"   channel="0" af="2"/>
      <spi name="MISO" channel="1" config="2" />
      <serial name="RX" channel="2" config="2" />
