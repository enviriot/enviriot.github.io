    <pin idx="4" fpin="10">
      <dio />
      <pwm timer="14"  channel="1" af="4"/>
      <ain name="AIN4" channel="4" />
    </pin>
    <pin idx="5" fpin="11">
      <dio />
      <pwm timer="2"   channel="1" af="5"/>
      <ain name="AIN5" channel="5" />
      <spi name="SCK" channel="1" config="1" />
