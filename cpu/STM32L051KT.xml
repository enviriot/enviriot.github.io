  <port name="GPIOB" titel="PB" offset="16" nr="1" pinset="GPIOB-&gt;BSRR = (1 &lt;&lt; {0})" pinrst="GPIOB-&gt;BRR = (1 &lt;&lt; {0})" pinget="GPIOB-&gt;IDR &amp; (1 &lt;&lt; {0})">
    <pin idx="0" fpin="14">
